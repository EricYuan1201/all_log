<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ylw.javaproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JavaProject">

        <!-- 测试菜单Activity作为启动页 -->
        <activity android:name=".TestMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 原始MainActivity -->
        <activity android:name=".MainActivity" />

        <!-- Java测试Activity -->
        <activity android:name=".TestJavaActivity" />

        <!-- Kotlin测试Activity -->
        <activity android:name=".TestKotlinActivity" />

        <!-- Lambda测试Activity -->
        <activity android:name=".TestLambdaActivity" />
    </application>

</manifest>